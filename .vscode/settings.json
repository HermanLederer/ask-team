{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"vue",
		"markdown",
		"svelte"
	],
	"javascript.suggestionActions.enabled": false,
	"typescript.suggestionActions.enabled": false
}
